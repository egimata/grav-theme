{% set title_level = title_level ?: 'h3' %}
{% if page.header.link %}
    <{{ title_level }}>
        {% if page.header.continue_link is not same as(false) %}
        <a href="{{ page.url }}"></a>
        {% endif %}
        <a href="{{ page.header.link }}">{{ page.title }}</a>
    </{{ title_level }}>
{% else %}
    <{{ title_level }} ><a href="{{ page.url }}">{{ page.title }}</a></{{ title_level }}>
{% endif %}
